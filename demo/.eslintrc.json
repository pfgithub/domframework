    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "plugins": ["jsx-a11y"],
    "extends": ["plugin:jsx-a11y/recommended"]
}
